{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "plugin:@next/next/recommended",
        "next"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": [
        "off"
      ],
      "react/jsx-uses-react": [
        "off"
      ],
      "react/no-unescaped-entities": [
        "off"
      ]
    }
}
